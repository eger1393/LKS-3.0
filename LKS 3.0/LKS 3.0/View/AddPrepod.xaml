<Window x:Class="LKS_3._0.AddPrepod"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LKS_3._0"
        mc:Ignorable="d"
        Title="Редактирование" Height="352.66" Width="310.008">

    <StackPanel>
        <Label x:Name="label" Content="Фамилия"/>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="{Binding MiddleName}"/>
        <Label x:Name="label1" Content="Имя"/>
        <TextBox x:Name="textBox1" Height="23" TextWrapping="Wrap" Text="{Binding FirstName}"/>
        <Label x:Name="label2" Content="Отчество"/>
        <TextBox x:Name="textBox2" Height="23" TextWrapping="Wrap" Text="{Binding LastName}"/>
        <Label x:Name="label3" Content="Звание"/>
        <TextBox x:Name="textBox3" Height="23" TextWrapping="Wrap" Text="{Binding Coolness}"/>
        <Label x:Name="label4" Content="Должность"/>
        <TextBox x:Name="textBox4" Height="23" TextWrapping="Wrap" Text="{Binding PrepodRank}"/>
        <Label x:Name="label5" Content="Дополнительно"/>
        <TextBox x:Name="textBox5" Height="23" TextWrapping="Wrap" Text="{Binding AdditionalInfo}"/>

        <Button x:Name="button" Content="Ok" Click="button_Click"/>


    </StackPanel>

</Window>
