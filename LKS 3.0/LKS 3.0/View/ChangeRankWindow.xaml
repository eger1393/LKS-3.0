<Window x:Class="LKS_3._0.View.ChangeRankWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LKS_3._0.View"
        mc:Ignorable="d"
        Title="Сменить должность" HorizontalAlignment="Center" VerticalAlignment="Center" Width="200" Height="150" WindowStartupLocation="CenterScreen">
    <StackPanel x:Name="stackPanel" Background="#FFD4D3D3">
        <Label x:Name="labelFIO" Margin="0,10,0,0" FontSize="16"/>
        <ComboBox x:Name="comboBoxRank" Margin="0,15,0,0" >
        
        </ComboBox>
        <Button x:Name="buttonOK" Content="Готово" Margin="0,15,0,0" Click="buttonOK_Click"/>

    </StackPanel>
</Window>
